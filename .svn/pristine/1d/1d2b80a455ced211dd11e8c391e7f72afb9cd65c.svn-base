@charset "UTF-8";



.mbrsh-card{
	width:354px;
	height:204px;
	border-radius:12px;
}
.card-tax{
	margin-left:20px;
	margin-right:10px
}
.second-mbrsh{
	text-align:right;
}

.second-card{
	text-align:justify;
}
.benetfitPrev {
	margin:30px 80px;
    display: flex;
}

.benetfitPrevList {
    width: 200px;
    margin: 25px 25px 25px 25px;
    padding: 17px 0 29px;
    border-top: 1px solid #bcbcbc;
}

.benetfitPrevList .tit {
	margin-bottom:10px;
    display: block;
    color: #eee;
    font: 400 18px notokrL;
    letter-spacing: -.075em;
}

.benetfitPrevList .tax {
    display: block;
    margin-bottom: 11px;
    color: #fff;
    font: 500 36px playfair,gothamB,notokrR;
    letter-spacing: .075em;
    font-feature-settings: 'lnum','pnum';
}

.benetfitPrevList .txt {
    display: block;
    color: #aaa;
    font: 400 16px gothamL,notokrL;
}

.mbsGrdNm {
    color: #fff;
    font: 500 28px playfair,gothamB,notokrR;
	
}

.mbsGrdAmnt {
    color: #fff;
    font: 500 24px playfair,gothamB,notokrR;
}

button span:hover {
	color:white;
}

.layerPop .layerCont {
    position: fixed;
    padding: 43px 70px 70px;
    background: #fff;
    z-index: 999999;
}

.layerCont .membershipConsult {
    position: relative;
    width: 940px;
}

.compTit {
    font: 500 34px/1.32 gothamB,notokrR;
    text-align: center;
    margin-bottom: 32px;
    letter-spacing: -.025em;
    text-transform: uppercase;
}

.layerCont .scrollWrap {
    margin: 0;
    padding: 0;
    height: 465px;
    padding-right: 8px !important;
    overflow-y: auto;
}

.layerPop .btnClose {
    overflow: hidden;
    text-indent: -9999px;
    position: absolute;
    top: 12px;
    right: 13px;
    width: 50px;
    height: 50px;
    z-index: 999;
}

.dimmed {
    display: none;
    position: fixed;
    top: 0;
    bottom: 0;
    left: 0;
    right: 0;
    background: #000;
    opacity: .95;
    z-index: 999998;
}