.custom-thead-bg{
	background-color : #efefef;
}

.custom-th-bg  tr th{
	background-color : #efefef;
}

.custom-body-table-bd{
	border-color: #cccccc;
	border-top: 3px solid #aaaaaa;

}

.custom-body-td-size {
    table-layout: fixed; /* 고정된 테이블 레이아웃을 사용합니다. */
    width: 100%; /* 테이블의 전체 너비를 100%로 설정합니다. */
    border-color: #cccccc
}

.custom-body-td-size td {
    width: 25%; /* 각 셀(`<td>`)의 너비를 25%로 설정합니다. */
}

.custom-modal-div-pd{
	padding-left:15px;
}

.custom-modal-row-pd{
	padding: 7px;
}

.inpWidth{
	width:50px;
}