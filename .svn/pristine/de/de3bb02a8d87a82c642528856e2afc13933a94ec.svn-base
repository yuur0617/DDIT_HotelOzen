@charset "UTF-8";

.modal-backdrop{
  position: fixed;
  top:0; left: 0; bottom: 0; right: 0;
  background-color:rgba(0, 0, 0, 0);
}

.mbrshImg {
        width: 300px;
        height: 100px;
    }

.mbrshContainer {
        text-align: center;
       
    }
    

hr.hidden {
        display: none;
    }

.checkbox {
	position: relative;
    left: -30%; 
    bottom: -5px;
}

@font-face {
    font-family: 'GmarketSansMedium';
    src: url('https://cdn.jsdelivr.net/gh/projectnoonnu/noonfonts_2001@1.1/GmarketSansMedium.woff') format('woff');
    font-weight: normal;
    font-style: normal;
}

.explain {
	font-family: 'GmarketSansMedium';
	font-size: 75%;
}

.membsh-title-span {
	font-family: 'GmarketSansMedium';
	font-size: 100%;
	cursor:hand;
	color:royalblue;
}

.membsh-span {
	cursor:hand;
	color:royalblue
}

.check-label {
	font-size: 75%;
}

.modal-dialog.modal-80size {
  width: 18%;
  height: 18%;
  margin: 0;
  padding: 0;
}

.modal-content.modal-80size {
  height: auto;
  min-height: 18%;
}

.modal_frame {
    position: fixed;
    left: 6%; 
    top: 12%; 
/*     transform: translate(-50%, -50%); */
}

#modal-end {
  padding: 3px 9px;
}
