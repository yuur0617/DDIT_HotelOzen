@charset "UTF-8";

.profile{
	margin-right:6px;
}

.profile > img {
	margin:10px auto;
	display:block;
	width:120px;
	height:120px;
	overflow: hidden;
	border: 5px solid #F6F5F5;
}

.rounded-circle-profile{
	border-radius:100%
}

.profile-dept{
	padding-bottom:10px;
}

.profile-button{
	background-color: #FFFFFF;
    border: 0;
    padding-left: 25%;
    color: black;
}

.profile-name-span{
	font-size: 20px;
	text-align: center;
}

.profile-dept-span{
	font-size:10px;
	text-align: center;
}
